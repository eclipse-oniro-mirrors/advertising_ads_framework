{
    "name": "@ohos/advertising",
    "description": "Allowing apps to use OS AD request and show ads",
    "version": "3.2",
    "license": "Apache License 2.0",
    "publishAs": "code-segment",
    "segment": {
      "destPath": "domains/cloud/advertising"
    },
    "dirs": {},
    "scripts": {},
    "component": {
      "name": "advertising",
      "subsystem": "cloud",
      "syscap": [
        "SystemCapability.Cloud.Ads"
      ],
      "features": [
      ],
      "adapted_system_type": [
        "standard"
      ],
      "rom":"300KB",
      "ram":"1024KB",
      "deps": {
        "components": [
          "ability_base",
          "ability_runtime",
          "bundle_framework",
          "c_utils",
          "config_policy",
          "ipc",
          "hilog",
          "napi",
          "safwk",
          "samgr"
        ],
        "third_party": [
          "jsoncpp",
          "libuv"
        ]
      },
      "build": {
        "sub_component": [
          "//domains/cloud/advertising:advertising_native_packages"
        ],
        "test": [
          "//domains/cloud/advertising/test/fuzztest:fuzztest"
        ]
      }
    }
  }
  